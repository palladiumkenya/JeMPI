        "/golden-record/:uid",
        "/golden-record/:uid/linked-records"
      ],
      "accessLevel": []
    }
  ],
  "rules": {
    "deterministic": {
      "QUERY_DETERMINISTIC_GOLDEN_RECORD_CANDIDATES": {
        "vars": ["nupi"],
        "text": "eq(nupi)"
      }
    },
    "probabilistic": {
      "QUERY_MATCH_GOLDEN_RECORD_CANDIDATES_BY_DISTANCE": {
        "vars": ["phonetic_given_name", "phonetic_family_name", "dob"],
        "text": "eq(phonetic_given_name) and eq(phonetic_family_name) or eq(phonetic_given_name) and match(dob,3) or eq(phonetic_family_name) and match(dob,3)"
      },
      "QUERY_MATCH_GOLDEN_RECORD_CANDIDATES_BY_NUPI": {
        "vars": ["nupi"],
        "text": "match(nupi,3)"
      }
    }
  }
}
